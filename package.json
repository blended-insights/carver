  "scripts": {
    "api:build": "npx nx run api:build --configuration=production",
    "api:dev": "npx nx run api:serve --configuration=development",
    "dev": "npx nx run-many --target=dev --projects=watcher,web",
    "docker-compose:dev": "docker-compose up neo4j redis",
    "docker-compose:up": "docker-compose -f 'docker-compose.yml' up -d --build ",
    "mcp:dev": "npx nx run mcp:serve --configuration=development",
    "mcp:dev:verbose": "npx nx run mcp:serve --configuration=development",
    "mcp:start": "npx @modelcontextprotocol/inspector node apps/mcp/dist/main.js",
    "mcp:start:prod": "NODE_ENV=production node apps/mcp/dist/main.js --host 0.0.0.0 --port 9230",
    "web:build": "NODE_ENV=production npx nx run web:build --prod",
    "web:dev": "npx nx run web:dev",
    "web:start": "npx nx run web:start --prod"
  },
  "private": true,
